---
import Layout from "../../layouts/Layout.astro";
import Plants from "../../components/Divisions/Plants.astro";
import About from "../../components/Divisions/About.astro";
import Banner from "../../components/Divisions/Banner.astro";
import Services from "../../components/Divisions/Services.astro";
import Contact from "../../components/Contact.astro";
import Iso from "../../components/Iso.astro";

import t from "../../lang/en.json"; 
const lang = 'en';
const base = '/en';
---

<Layout lang={lang} base={base} t={t} title="Tooling Division - KZ Group" description="" keywords="" pageId="matriceria">
    <Banner bannerId="matriceria" t={t.matriceria.banner}/>
    <About aboutId="matriceria" t={t.matriceria.about}/>
    <Services servicesId="matriceria" t={t.matriceria.services}/>
    <Plants t={t.matriceria.plants} />
    <Contact t={t.main.contact}/>
    <Iso base={base} isoId="matriceria" t={t.main.iso}/>
</Layout>
