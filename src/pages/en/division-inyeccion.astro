---
import Layout from "../../layouts/Layout.astro";
import Plants from "../../components/Divisions/Plants.astro";
import About from "../../components/Divisions/About.astro";
import Banner from "../../components/Divisions/Banner.astro";
import Services from "../../components/Divisions/Services.astro";
import Contact from "../../components/Contact.astro";
import Iso from "../../components/Iso.astro";

import t from "../../lang/en.json"; 
const lang = 'en';
const base = '/en';
---

<Layout lang={lang} base={base} t={t} title="División Inyección - KZ Group" description="" keywords="" pageId="inyeccion">
    <Banner bannerId="inyeccion" t={t.inyeccion.banner}/>
    <About aboutId="inyeccion" t={t.inyeccion.about}/>
    <Services servicesId="inyeccion" t={t.inyeccion.services}/>
    <Plants t={t.inyeccion.plants} />
    <Contact t={t.main.contact}/>
    <Iso base={base} isoId="inyeccion" t={t.main.iso}/>
</Layout>
